{
    "name": "sobirjonovs/laravel-rabbit",
    "description": "Easy tool for working with RabbitMQ",
    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": "^7.3|^8.0",
        "php-amqplib/php-amqplib": "^3.1",
        "illuminate/support": "~8"
    },
    "homepage": "https://github.com/sobirjonovs/laravel-rabbit",
    "keywords": ["RabbitMQ", "Laravel rabbit-mq", "rabbit", "client"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\Rabbitmq\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Sanjarbek Sobirjonov",
            "email": "sobirjonovsdev@gmail.com",
            "homepage": "https://github.com/sobirjonovs"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "App\\Rabbitmq\\Providers\\RabbitServiceProvider"
            ],
            "aliases": {
                "Rabbitmq": "App\\Rabbitmq\\Facades\\RabbitmqFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
